{
  "name": "shawnmccool/dot-filer",
  "description": "A PHP script to help manage dot files in a git repository.",
  "type": "project",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawnmc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "DotFiler\\": "src/"
    },
    "files": [
      "./src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DotFiler\\": "tests/"
    },
    "files": [
      
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
