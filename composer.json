{
  "name": "shawnmccool/dot-filer",
  "description": "A tool to backup and restore dot files.",
  "type": "project",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawnmc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ulrichsg/getopt-php": "dev-master",
    "phpunit/phpunit": "^9"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "DotFiler\\": "src/"
    },
    "files": [
      "./src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\DotFiler\\": "tests/"
    },
    "files": [
      
    ]
  },
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist"
  }
}
